{
  "manifest_version": 2,
  "name": "CardinalAI Web",
  "version": "1.0.0",
  "description": "Acceso r√°pido a CardinalAI Web",
  "icons": {
    "256": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "256": "icon.png"
    },
    "default_title": "CardinalAI Web"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "cardinal-ai.html"
  },
  "permissions": [
    "alarms",
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
